{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:stg": "vite --mode stg",
    "build": "vite build --mode stg",
    "build:prod": "vite build --mode production",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview:stg": "vite preview --mode stg",
    "typecheck": "tsc --noEmit -p tsconfig.app.json",
    "server": "npx supabase functions serve",
    "db:types": "npx supabase gen types typescript --local > src/lib/database.types.ts && cp src/lib/database.types.ts supabase/functions/_shared/database.types.ts",
    "db:types:remote": "npx supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > src/lib/database.types.ts && cp src/lib/database.types.ts supabase/functions/_shared/database.types.ts",
    "import-wallets:stg": "node -r dotenv/config scripts/import-wallets-from-csv.cjs dotenv_config_path=.env.stg"
  },
  "dependencies": {
    "@nktkas/hyperliquid": "^0.25.7",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@supabase/supabase-js": "^2.57.4",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/react-query": "^5.90.9",
    "@tanstack/react-table": "^8.21.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "csv-parse": "^6.1.0",
    "ethers": "^6.11.1",
    "lucide-react": "^0.344.0",
    "motion": "^12.23.24",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.9.5",
    "supabase": "^2.54.11",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "tailwindcss-animate": "^1.0.7",
    "wouter": "^3.7.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/node": "^24.10.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "dotenv": "^17.2.3",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "tailwind-animate": "^0.2.10",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
